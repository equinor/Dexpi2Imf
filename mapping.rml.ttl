:ConceptModelMap a rr:TriplesMap;
    rml:logicalSource [
    rml:source "pandid.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/PlantModel"
  ].

:ConceptModelMap rr:subjectMap [
    rr:template "https://example.com/PlantModel/{@ID}"
  ].